{
  "name": "@pi/account-pages",
  "version": "1.0.0",
  "description": "Account frontend for Xeneta",
  "private": true,
  "license": "MIT",
  "author": "Xeneta",
  "exports": {
    ".": "./src",
    "./*": "./src/*",
    "./**": "./src/**",
    "./**/*": "./src/**/*",
    "./config/*": "./config/*.js",
    "./package.json": "./package.json"
  },
  "main": "src/index.tsx",
  "scripts": {
    "build:dev": "webpack --config './webpack.config.js' --env environment=development --mode development",
    "build:prod": "webpack --config './webpack.config.js' --env environment=production --mode production",
    "lint": "pnpm lint:ts && pnpm lint:styles",
    "lint:styles": "stylelint 'src/**/*.{ts,tsx}'",
    "lint:ts": "eslint './src' --ext '.ts,.tsx'",
    "serve": "webpack serve --config ./webpack.config.js --env environment=development --mode development"
  },
  "keywords": [],
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^16",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.2.0",
    "babel-loader": "^8.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "ts-loader": "^9.1.1",
    "typescript": "^5.0.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@pi/ui": "workspace:*",
    "@reduxjs/toolkit": "^1.9.3",
    "isomorphic-fetch": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-is": "^18.2.0",
    "react-redux": "7.2.9",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0"
  }
